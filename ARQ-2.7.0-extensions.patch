+	 * select * where {
+	 *   { ?s a foaf:Person ; foaf:name ?name . FILTER (?name = ?name2) }
+	 *   UNION
+	 *   { ?s a bsbm:Product ; rdfs:label ?name2 . FILTER (?name = ?name2) }
+	 * }
