+				V_e = e.getVarsMentioned();
+				V_r = OpVars.allVars(opJoin.getRight());
+				V_e.removeAll(V_r);
+				rightSubset = V_e.size() == 0;
+				
+				if (leftSubset && canPushDown(e))
+					leftExprs.add(e);
+				if (rightSubset && canPushDown(e))
+					rightExprs.add(e);
+				if (!leftSubset && !rightSubset)
