+			"SELECT  * \n" +
+			"WHERE { \n" +
+			"  { ?a ?c [] }\n" +
+			"  { ?a ?b ?d }\n" +
+			"  FILTER ( ?a = 1 && ?b = 2 && ?c = ?d && bound(?a)) }\n";
