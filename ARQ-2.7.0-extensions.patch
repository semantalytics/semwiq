+			}
+			
+			// insert filters below join
+			if (leftExprs.size() > 0)
+				opJoin.setLeft(OpFilter.filter(leftExprs, opJoin.getLeft()));
+			if (rightExprs.size() > 0)
+				opJoin.setRight(OpFilter.filter(rightExprs, opJoin.getRight()));
