<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:protege="http://protege.stanford.edu/plugins/owl/protege#"
    xmlns="http://semwiq.faw.uni-linz.ac.at/vocab/global/science/astronomy/solar-observation.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:person="http://daml.umbc.edu/ontologies/ittalks/person#"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vcard="http://www.w3.org/2006/vcard/ns#"
    xmlns:obs="http://semwiq.faw.uni-linz.ac.at/vocab/global/science/observation.owl#"
    xmlns:geo="http://www.w3.org/2003/01/geo/wgs84_pos#"
  xml:base="http://semwiq.faw.uni-linz.ac.at/vocab/global/science/astronomy/solar-observation.owl">
  <owl:Ontology rdf:about="">
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >v1.0 2008-04-04</owl:versionInfo>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Used to describe solar observations</rdfs:comment>
    <owl:imports rdf:resource="http://semwiq.faw.uni-linz.ac.at/vocab/global/science/observation.owl"/>
  </owl:Ontology>
  <owl:Class rdf:ID="SunExposure">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >sun exposure</rdfs:label>
    <rdfs:subClassOf>
      <owl:Class rdf:ID="SolarObservation"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://semwiq.faw.uni-linz.ac.at/vocab/global/science/observation.owl#Exposure"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >exposure of solar phenomenons, usually stored as image data, e.g. as FITS file or JPEG</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="SolarObservatory">
    <rdfs:subClassOf rdf:resource="http://semwiq.faw.uni-linz.ac.at/vocab/global/science/observation.owl#Observatory"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a scientific institute which is observing solar phenomenons</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >solar observatory</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="SolarObservationInstrument">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >an instrument for observing solar phenomenons</rdfs:comment>
    <rdfs:subClassOf rdf:resource="http://semwiq.faw.uni-linz.ac.at/vocab/global/science/observation.owl#ObservationInstrument"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >solar observation instrument</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Detector">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >solar observation instrument detector</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a detector which is used by solar observation instruments</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="serialAxis"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="parallelAxis"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
  </owl:Class>
  <owl:Class rdf:ID="Filter">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a filter which is used by solar observation instruments</rdfs:comment>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="waveUnit"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="waveLength"/>
        </owl:onProperty>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="fullWithAtHalfMax"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#int"
        >1</owl:cardinality>
        <owl:onProperty>
          <owl:FunctionalProperty rdf:ID="equivWidth"/>
        </owl:onProperty>
      </owl:Restriction>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >solar observation instrument filter</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="SunspotRelativeNumbers">
    <rdfs:subClassOf>
      <owl:Class rdf:about="#SolarObservation"/>
    </rdfs:subClassOf>
    <rdfs:subClassOf rdf:resource="http://semwiq.faw.uni-linz.ac.at/vocab/global/science/observation.owl#InstantObservation"/>
    <rdfs:subClassOf rdf:resource="http://semwiq.faw.uni-linz.ac.at/vocab/global/science/observation.owl#AbstractData"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >sunspot relative numbers</rdfs:label>
    <rdfs:comment xml:lang="de">Die Sonnenflecken-Relativzahlen sind f체r eine ungef채hre Charakterisierung der Sonnenaktivit채t wichtig. Obwohl die physikalische Bedeutung begrenzt ist, zeigen sie eine gute Korrelation mit anderen physikalischen Parametern der Sonnenaktivit채t. Die Bestimmung der Relativzahl ist sehr einfach und daher existieren lange Beobachtungsreihen.</rdfs:comment>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >relative numbers of observed sunspots at a given instant (usually observed in a daily synoptic series)</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:about="#SolarObservation">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >observation of solar phenomenons</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >solar observation</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://semwiq.faw.uni-linz.ac.at/vocab/global/science/observation.owl#ObservationByPhenomenon"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:allValuesFrom rdf:resource="#SolarObservationInstrument"/>
        <owl:onProperty rdf:resource="http://semwiq.faw.uni-linz.ac.at/vocab/global/science/observation.owl#instrument"/>
      </owl:Restriction>
    </rdfs:subClassOf>
  </owl:Class>
  <owl:Class rdf:ID="SunspotDrawing">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >sunspot drawing</rdfs:label>
    <rdfs:subClassOf rdf:resource="#SolarObservation"/>
    <rdfs:subClassOf rdf:resource="http://semwiq.faw.uni-linz.ac.at/vocab/global/science/observation.owl#AbstractDrawing"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >abstract drawing of sunspots, usually showing spots and groups of spots</rdfs:comment>
  </owl:Class>
  <owl:FunctionalProperty rdf:about="#parallelAxis">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >parallel axis</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >the parallel axis of a solar observation instrument detector</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="filter">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:range rdf:resource="#Filter"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >specifies a filter used by solar observation instruments</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >solar observation instrument filter</rdfs:label>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#serialAxis">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >the serial axis of a solar observation instrument detector</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >serial axis</rdfs:label>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#waveLength">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >wave length</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >wave length</rdfs:label>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#equivWidth">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >equivalent width</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >equivalent width of a filter, bandwidth (used for radio observations)</rdfs:comment>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#waveUnit">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >specifies a wave unit (value should be some strong identifyer, e.g. dbpedia URI, a popular SKOS concept, etc.)</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >wave unit</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="xCenter">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >x center</rdfs:label>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdfs:domain rdf:resource="#SunExposure"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >x centering of solar exposure</rdfs:comment>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="yCenter">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >y centering of solar exposure</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >y center</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:domain rdf:resource="#SunExposure"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="spots">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#SunspotRelativeNumbers"/>
    <rdfs:seeAlso rdf:resource="http://www.astro.physik.uni-goettingen.de/~hessman/rdf/IAU93#SunspotNumbers"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >sunspots</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >number of observable sunspots</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="detector">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >solar observation instrument detector</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >specifies a detector used by solar observation instruments</rdfs:comment>
    <rdfs:range rdf:resource="#Detector"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:about="#fullWithAtHalfMax">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >full width at half max</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >full width at half max (see also http://dbpedia.org/resource/Full_width_at_half_maximum)</rdfs:comment>
    <rdfs:seeAlso rdf:resource="http://dbpedia.org/resource/Full_width_at_half_maximum"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="seeingQuality">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#SunspotRelativeNumbers"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >seeing quality</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >the current visibility or sky clearness (Kiepenheuer's scale)</rdfs:comment>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="rValue">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >r-value</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >r-value, calcualted as R = k(10g+f) where k is a correction factor (obtained from current sky clearness/visibility and the used instrument), g is the number of groups and f the number of observable spots (umbrae).</rdfs:comment>
    <rdfs:domain rdf:resource="#SunspotRelativeNumbers"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="groups">
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >sunspot groups</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >number of observable sunspot groups</rdfs:comment>
    <rdfs:seeAlso rdf:resource="http://www.astro.physik.uni-goettingen.de/~hessman/rdf/IAU93#SunspotGroups"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#int"/>
    <rdfs:domain rdf:resource="#SunspotRelativeNumbers"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
</rdf:RDF>

