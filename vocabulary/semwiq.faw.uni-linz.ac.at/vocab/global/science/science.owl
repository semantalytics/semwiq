<?xml version="1.0"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:swap-pim="http://www.w3.org/2000/10/swap/pim/contact#"
    xmlns:dc-protege="http://protege.stanford.edu/plugins/owl/dc/protege-dc.owl#"
    xmlns="http://semwiq.faw.uni-linz.ac.at/vocab/global/science/science.owl#"
    xmlns:owl="http://www.w3.org/2002/07/owl#"
    xmlns:wordnet="http://xmlns.com/wordnet/1.6/"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:vcard="http://www.w3.org/2006/vcard/ns#"
    xmlns:geo="http://www.w3.org/2003/01/geo/wgs84_pos#"
  xml:base="http://semwiq.faw.uni-linz.ac.at/vocab/global/science/science.owl">
  <owl:Ontology rdf:about="">
    <owl:imports rdf:resource="http://www.w3.org/2006/vcard/ns"/>
    <owl:imports rdf:resource="http://xmlns.com/foaf/0.1/"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >Basic science ontology. Can be used to describe scientific organizations, institutes, and scientists.</rdfs:comment>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >v1.0 2008-04-04</owl:versionInfo>
  </owl:Ontology>
  <owl:Class rdf:ID="ScientificInstrument">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    ></rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >scientific  instrument</rdfs:label>
  </owl:Class>
  <owl:Class rdf:ID="Scientist">
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >scientist</rdfs:label>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >a person who is a scientist</rdfs:comment>
  </owl:Class>
  <owl:Class rdf:ID="ScientificInstitute">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >an academic or industrial institute which is dedicated to do research</rdfs:comment>
    <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string"
    >scientific institute</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://xmlns.com/foaf/0.1/Organization"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2003/01/geo/wgs84_pos#SpatialThing"/>
  </owl:Class>
  <owl:ObjectProperty rdf:ID="contactPerson">
    <rdfs:range rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
  </owl:ObjectProperty>
  <owl:DatatypeProperty rdf:ID="position">
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
  </owl:DatatypeProperty>
  <owl:FunctionalProperty rdf:ID="locatedAt">
    <rdfs:range rdf:resource="http://www.w3.org/2006/vcard/ns#Address"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
  </owl:FunctionalProperty>
  <owl:FunctionalProperty rdf:ID="retired">
    <rdfs:domain rdf:resource="http://xmlns.com/foaf/0.1/Person"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#DatatypeProperty"/>
  </owl:FunctionalProperty>
</rdf:RDF>

